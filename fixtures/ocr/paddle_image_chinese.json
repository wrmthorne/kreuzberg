{
	"id": "ocr_paddle_image_chinese",
	"category": "ocr",
	"description": "Chinese OCR with PaddleOCR - its core strength.",
	"document": {
		"path": "images/chi_sim_image.jpeg",
		"media_type": "image/jpeg",
		"requires_external_tool": ["paddle-ocr", "onnxruntime"]
	},
	"extraction": {
		"config": {
			"ocr": {
				"backend": "paddle-ocr",
				"language": "ch"
			},
			"force_ocr": true
		}
	},
	"assertions": {
		"expected_mime": "image/jpeg",
		"min_content_length": 1
	},
	"skip": {
		"requires_feature": ["paddle-ocr"],
		"notes": "Requires PaddleOCR with Chinese models"
	}
}
