{
	"id": "ocr_paddle_structured",
	"category": "ocr",
	"description": "PaddleOCR with structured output preserving all metadata.",
	"document": {
		"path": "images/test_hello_world.png",
		"media_type": "image/png",
		"requires_external_tool": ["paddle-ocr", "onnxruntime"]
	},
	"extraction": {
		"config": {
			"ocr": {
				"backend": "paddle-ocr",
				"language": "en",
				"element_config": {
					"include_elements": true
				}
			},
			"force_ocr": true
		}
	},
	"assertions": {
		"expected_mime": "image/png",
		"min_content_length": 5,
		"ocr_elements": {
			"has_elements": true,
			"elements_have_geometry": true,
			"elements_have_confidence": true
		}
	},
	"skip": {
		"requires_feature": ["paddle-ocr"],
		"notes": "Tests structured output with bbox/confidence preservation"
	}
}
