{
	"id": "ocr_paddle_confidence_filter",
	"category": "ocr",
	"description": "PaddleOCR with minimum confidence threshold filtering.",
	"document": {
		"path": "images/ocr_image.jpg",
		"media_type": "image/jpeg",
		"requires_external_tool": ["paddle-ocr", "onnxruntime"]
	},
	"extraction": {
		"config": {
			"ocr": {
				"backend": "paddle-ocr",
				"language": "en",
				"paddle_ocr_config": {
					"min_confidence": 80.0
				}
			},
			"force_ocr": true
		}
	},
	"assertions": {
		"expected_mime": "image/jpeg",
		"min_content_length": 1
	},
	"skip": {
		"requires_feature": ["paddle-ocr"],
		"notes": "Tests confidence threshold filtering with PaddleOCR"
	}
}
