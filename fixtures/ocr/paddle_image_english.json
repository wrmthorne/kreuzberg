{
	"id": "ocr_paddle_image_english",
	"category": "ocr",
	"description": "Simple English image OCR with PaddleOCR backend.",
	"document": {
		"path": "images/test_hello_world.png",
		"media_type": "image/png",
		"requires_external_tool": ["paddle-ocr", "onnxruntime"]
	},
	"extraction": {
		"config": {
			"ocr": {
				"backend": "paddle-ocr",
				"language": "en"
			},
			"force_ocr": true
		}
	},
	"assertions": {
		"expected_mime": "image/png",
		"min_content_length": 5,
		"content_contains_any": ["hello", "Hello", "world", "World"]
	},
	"skip": {
		"requires_feature": ["paddle-ocr"],
		"notes": "Requires PaddleOCR with ONNX Runtime"
	}
}
