{
  "name": "@kreuzberg/test-utils",
  "version": "4.0.8",
  "description": "Shared test utilities for Kreuzberg e2e and integration tests",
  "author": {
    "name": "Na'aman Hirschfeld",
    "email": "nhirschfeld@gmail.com",
    "url": "https://kreuzberg.dev"
  },
  "homepage": "https://kreuzberg.dev",
  "bugs": {
    "url": "https://github.com/kreuzberg-dev/kreuzberg/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kreuzberg-dev/kreuzberg.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./config-mapping": {
      "types": "./dist/config-mapping/index.d.ts",
      "import": "./dist/config-mapping/index.mjs",
      "require": "./dist/config-mapping/index.js"
    },
    "./assertions": {
      "types": "./dist/assertions/index.d.ts",
      "import": "./dist/assertions/index.mjs",
      "require": "./dist/assertions/index.js"
    },
    "./fixtures": {
      "types": "./dist/fixtures/index.d.ts",
      "import": "./dist/fixtures/index.mjs",
      "require": "./dist/fixtures/index.js"
    },
    "./paths": {
      "types": "./dist/paths/index.d.ts",
      "import": "./dist/paths/index.mjs",
      "require": "./dist/paths/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">= 22"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "prepare": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "biome check src",
    "lint:fix": "biome check --write src"
  },
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.0.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
